numeros = input("Digite uma lista de números separados por espaço: ").split()
numeros = [int(numero) for numero in numeros]

maior_n = numeros[0]
menor_n = numeros[0]

for numero in numeros:
    if numero > maior_n:
        maior_n = numero
    elif numero < menor_n:
        menor_n = numero

print(f"O maior número é: {maior_n}")
print(f"O menor número é: {menor_n}")
